{
  "name": "workshop.zfleet_workshop",
  "version": "0.0.1",
  "private": true,
  "sapuxLayer": "CUSTOMER_BASE",
  "description": "",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "scripts": {
    "start": "node --trace-deprecation index.js",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandboxMockServer.html?sap-client=100&sap-ui-xx-viewCache=false#app-tile\"",
    "start-noflp": "fiori run --open \"index.html?sap-client=100&sap-ui-xx-viewCache=false\"",
    "start-variants-management": "fiori run --open \"preview.html?sap-client=100&sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
    "build": "ui5 build -a --clean-dest --include-task=generateCachebusterInfo",
    "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml",
    "deploy-config": "fiori add deploy-config",
    "start-mock": "fiori run --config ./ui5-mock.yaml --open \"test/flpSandbox.html?sap-client=100&sap-ui-xx-viewCache=false#app-tile\"",
    "suite-tests": "fiori run --open \"test/testsuite.qunit.html\"",
    "unit-tests": "fiori run --open \"test/unit/unitTests.qunit.html\"",
    "int-tests": "fiori run --open \"test/integration/opaTests.qunit.html\"",
    "undeploy": "npm run build && fiori undeploy --config ui5-deploy.yaml",
    "deploy-test": "npm run build && fiori deploy --config ui5-deploy.yaml --testMode true"
  },
  "devDependencies": {
    "@sap-ux/ui5-middleware-fe-mockserver": "2",
    "@sap/ux-ui5-tooling": "1",
    "@ui5/cli": "^3.0.0",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "@sap-ux/adp-tooling": "^0.15.37",
    "@sap-ux/preview-middleware": "^0.23.21",
    "external-editor": "^1.1.0"
  }
}